<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dao.EnrolStudentsToCourse" table="ENROL_STUDENTS_TO_COURSE">
        <id name="id" type="int" column="ID">
            <generator class="native"/>
        </id>
        <many-to-one name="courseID" fetch="select" lazy="false" column="courseID" unique="true" class="dao.Course"/>
        <many-to-one name="studentID" fetch="select" lazy="false" column="studentID" unique="true"
                     class="dao.Student"/>
        <property name="userRole" column="userRole" type="string"/>
    </class>
</hibernate-mapping>