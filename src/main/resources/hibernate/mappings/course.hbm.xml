<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dao.Course" table="Course">
        <meta attribute="class-description">
            This class contains courses details.
        </meta>
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="name" column="name" type="string"/>
        <property name="code" column="code" type="int"/>
        <property name="responsible" column="TEACHER" type="string"/>
        <property name="oneId" column="oneId" type="int"/>
    </class>
</hibernate-mapping>